{
  "name": "launchpad",
  "version": "1.0.0",
  "author": "",
  "description": "",
  "keywords": [],
  "license": "ISC",
  "scripts": {
    "__build:vendors__": "[ -f webpack.vendors.ts ] && NODE_OPTIONS=--max-old-space-size=4096 webpack --config webpack.vendors.ts",
    "analyze": "yarn run --silent build -p --profile --json > ./stats.json && webpack-bundle-analyzer ./stats.json ./dist/ --mode static",
    "build": "webpack -p --display-error-details",
    "clean": "rm -rf ./dist; rm -rf ./report.html; rm -rf ./stats.json; rm -rf ./storybook-output;",
    "freeze": "yarn install --frozen-lockfile",
    "lint": "yarn run lint:css && yarn run lint:js",
    "lint:css": "stylelint 'src/**/*.scss'",
    "lint:js": "eslint --ext .js,.ts,.tsx --no-error-on-unmatched-pattern src",
    "preanalyze": "yarn run freeze",
    "presetup": "yarn run freeze",
    "prestart": "yarn run stop 2> /dev/null; ([ -z \"$(yarn run freeze 2> /dev/null | grep 'Already up-to-date')\" ] || [ ! -f ./public/vendors-manifest.json ]) && yarn run __build:vendors__ 2> /dev/null || true",
    "purge": "yarn run purge:docker; yarn run purge:git; yarn run clean",
    "serve": "ts-node ./src/mocks/express",
    "setup": "yarn run __build:vendors__",
    "start": "webpack-dev-server",
    "stop": "lsof -i \":${PORT:-8080}\" | grep 'LISTEN' | awk '{ print $2; }' | xargs kill 2> /dev/null || true",
    "test": "jest",
    "typecheck": "tsc",
    "update": "npm-check-updates -u '/^(?!'\"$(node -p 'Object.keys(require(\"./package.json\").resolutions).join(\"$|\")')\"'$).*/' && rm -rf node_modules yarn.lock && yarn install",
    "wait": "printf 'Waiting for UI (%s)...' \"${HOST:-\"https://localhost:8080\"}\"; while [ '{\"status\":\"healthy\"}' != \"$(curl -sk \"${HOST:-\"https://localhost:8080\"}/health-check\" 2> /dev/null)\" ]; do sleep 1; done; echo 'ready.'"
  },
  "resolutions": {
    "@types/webpack": "4",
    "copy-webpack-plugin": "6",
    "favicons-webpack-plugin": "4",
    "html-webpack-plugin": "4",
    "html-webpack-tags-plugin": "3",
    "husky": "4",
    "mini-css-extract-plugin": "0.12",
    "sass-loader": "10",
    "source-map-loader": "1",
    "terser-webpack-plugin": "4",
    "webpack": "4",
    "webpack-cli": "3"
  },
  "dependencies": {
    "clsx": "^1.1.1",
    "debug": "^4.3.1",
    "msw": "^0.26.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-query": "^3.9.8",
    "react-router-dom": "^5.2.0",
    "styled-components": "^5.2.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@pmmmwh/react-refresh-webpack-plugin": "^0.4.3",
    "@svgr/webpack": "^5.5.0",
    "@testing-library/react": "^11.2.5",
    "@types/body-parser": "^1.19.0",
    "@types/compression": "^1.7.0",
    "@types/copy-webpack-plugin": "^6.4.0",
    "@types/express": "^4.17.11",
    "@types/node": "^14.14.31",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "@types/react-router-dom": "^5.1.7",
    "@types/testing-library__react": "^10.2.0",
    "@types/webpack": "4",
    "@types/webpack-dev-server": "^3.11.1",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "body-parser": "^1.19.0",
    "clean-webpack-plugin": "^3.0.0",
    "compression": "^1.7.4",
    "copy-webpack-plugin": "6",
    "css-loader": "^5.0.2",
    "dotenv": "^8.2.0",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^8.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "express": "^4.17.1",
    "file-loader": "^6.2.0",
    "fork-ts-checker-webpack-plugin": "^6.1.0",
    "html-webpack-plugin": "4",
    "html-webpack-tags-plugin": "3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "mini-css-extract-plugin": "0.12",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "preact": "^10.5.12",
    "prettier": "^2.2.1",
    "qs": "^6.9.6",
    "react-dev-utils": "^11.0.2",
    "react-refresh": "^0.9.0",
    "sass": "^1.32.8",
    "sass-loader": "10",
    "selfsigned": "^1.10.8",
    "source-map-loader": "1",
    "style-loader": "^2.0.0",
    "stylelint": "^13.11.0",
    "stylelint-config-sass-guidelines": "^8.0.0",
    "terser-webpack-plugin": "4",
    "ts-jest": "^26.5.1",
    "ts-loader": "^8.0.17",
    "ts-node": "^9.1.1",
    "tsconfig-paths-webpack-plugin": "^3.3.0",
    "typescript": "^4.1.5",
    "typescript-plugin-css-modules": "^3.2.0",
    "webpack": "4",
    "webpack-bundle-analyzer": "^4.4.0",
    "webpack-cli": "3",
    "webpack-dev-server": "^3.11.2"
  },
  "optionalDependencies": {
    "husky": "4"
  },
  "husky": {
    "hooks": {
      "pre-push": "echo 'Type Checking...' && yarn run typecheck && echo 'Lint Checking...' && yarn run lint"
    }
  }
}
